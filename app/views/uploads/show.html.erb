<div class="col-md-4 offset-md-4 bg-white rounded mt-5 p-5">

<h2>This is upload show</h2>
<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @upload.title %>
</p>

<p>
  <strong>Description:</strong>
  <%= @upload.description %>
</p>

<p>
  <strong>Is public:</strong>
  <%= @upload.is_public %>
</p>

<p>
  <strong>User:</strong>
  <%= @upload.user %>
</p>
<div class="">
    <img src="<%= @upload.photoupload %>" alt="<%= @upload.title %>" class="upload_img">
</div>


<%= link_to 'Edit', edit_upload_path(@upload) %> |
<%= link_to 'Back', uploads_path %>
</div>
